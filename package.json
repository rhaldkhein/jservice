{
  "name": "jservice",
  "version": "0.3.1",
  "description": "A small and powerful pure javascript DI container",
  "main": "lib/index.js",
  "scripts": {
    "build": "rm -rf lib && babel src --out-dir lib",
    "watch": "rm -rf lib && babel src --watch --out-dir lib",
    "sample": "DEBUG=jservice:* node sample/basic",
    "start": "DEBUG=jservice:* nodemon sample/basic",
    "prepare": "npm run build",
    "test": "nyc mocha",
    "test-watch": "mocha --watch",
    "codecov": "nyc report --reporter=text-lcov > coverage.lcov && codecov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rhaldkhein/jservice.git"
  },
  "author": "RhaldKhein",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rhaldkhein/jservice/issues"
  },
  "homepage": "https://github.com/rhaldkhein/jservice#readme",
  "keywords": [
    "ioc",
    "dependency-injection",
    "di",
    "container",
    "services"
  ],
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.5",
    "babel-eslint": "^10.0.1",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "codecov": "^3.5.0",
    "eslint": "^5.16.0",
    "express": "^4.17.1",
    "fastify": "^2.6.0",
    "koa": "^2.7.0",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "restana": "^3.2.2",
    "shortid": "^2.2.14"
  },
  "files": [
    "src",
    "lib"
  ]
}
